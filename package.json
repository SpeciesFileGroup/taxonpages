{
  "name": "@sfgrp/taxonpages",
  "version": "0.1.2",
  "description": "CLI tool for building taxon pages powered by SpeciesFileGroup",
  "type": "module",
  "private": false,
  "bin": {
    "taxonpages": "./bin/taxonpages.js"
  },
  "files": [
    "bin/",
    "src/",
    "templates/",
    "server.js",
    "vite.config.js",
    "index.html",
    "tailwind.config.cjs",
    "postcss.config.cjs"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "node bin/taxonpages.js dev",
    "dev:ssr": "node bin/taxonpages.js dev:ssr",
    "build": "node bin/taxonpages.js build",
    "build:ssr": "node bin/taxonpages.js build:ssr",
    "serve": "node bin/taxonpages.js serve",
    "debug": "node --inspect-brk server",
    "preview": "node bin/taxonpages.js preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.19.2",
    "@nuxt/devalue": "^2.0.2",
    "@sfgrp/distinguish": "^0.4.2",
    "@sfgrp/pinpoint": "^0.0.12",
    "@tailwindcss/typography": "^0.5.19",
    "@unhead/ssr": "^2.1.4",
    "@unhead/vue": "^2.1.4",
    "@vitejs/plugin-vue": "^6.0.4",
    "autoprefixer": "^10.4.24",
    "axios": "^1.13.5",
    "commander": "^14.0.3",
    "compression": "^1.8.1",
    "express": "^5.2.1",
    "glob": "^13.0.3",
    "js-yaml": "^4.1.1",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "markdown-it-anchor": "^9.2.0",
    "minimist": "^1.2.8",
    "pinia": "^3.0.4",
    "postcss": "^8.5.6",
    "sass": "^1.97.3",
    "serve-static": "^2.2.1",
    "tailwindcss": "^3.4.17",
    "unplugin-vue-markdown": "^29.2.0",
    "vite": "^7.3.1",
    "vite-plugin-pages": "^0.33.3",
    "vite-plugin-radar": "^0.10.1",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0"
  },
  "keywords": [
    "taxonworks",
    "biodiversity",
    "taxonomy",
    "vue",
    "ssr",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SpeciesFileGroup/taxonpages.git"
  }
}
